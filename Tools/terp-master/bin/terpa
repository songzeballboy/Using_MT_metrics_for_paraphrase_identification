#!/bin/bash

#####################################################################
###
### EDIT THESE LINES APPROPRIATELLY
###
#####################################################################
PATH_TO_JAVA="/cygdrive/d/Java/jdk//bin/java"
PATH_TO_TER="./"
MEM_PAR="-Xms1G -Xmx3G"

#####################################################################
###
### DO NOT EDIT BELOW HERE
###
#####################################################################

${PATH_TO_JAVA} ${MEM_PAR} -jar ${PATH_TO_TER}/dist/lib/terp.jar -p ${PATH_TO_TER}/data/terpa.param -p ${PATH_TO_TER}/data/data_loc.param $@

exit $?
